    <script>
        function  assignSkill(instance, inputID) {
            var ele = document.getElementById(inputID);
            ele.focus();
            ele.value = '0';
            if(instance.checked){
                ele.focus();
                var initVal = ele.value;
                ele.value = Number(initVal) + 1;
            }
        }
        $('.hollow').click(function () {
            var iniVal = $(this).closest('.input-group').find('input').val();
            if($(this).attr('data-quantity') == 'minus' && Number(iniVal) > 0){
                $(this).closest('.input-group').find('input').val(Number(iniVal) - 1);
            }
            if($(this).attr('data-quantity') == 'plus'){
                $(this).closest('.input-group').find('input').val(Number(iniVal) + 1);
            }
        });
    </script>
